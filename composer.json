{
  "name": "symfony/framework-standard-edition",
  "license": "MIT",
  "type": "project",
  "description": "The \"Symfony Standard Edition\" distribution",
  "autoload": {
    "psr-4": {
      "AppBundle\\": "src/AppBundle",
      "Application\\": "src/Application"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "files": [
      "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
    ]
  },
  "require": {
    "php": ">=7.0",
    "braincrafted/bootstrap-bundle": "~2.0",
    "components/jquery": "1.11.1",
    "doctrine/doctrine-bundle": "1.8.0",
    "doctrine/doctrine-migrations-bundle": "1.1.*",
    "doctrine/migrations": "^1.0",
    "doctrine/orm": "^2.4.8",
    "egeloen/ckeditor-bundle": "^6.0",
    "friendsofsymfony/rest-bundle": "^1.8",
    "gedmo/doctrine-extensions": "^2.0",
    "incenteev/composer-parameter-handler": "~2.0",
    "jms/serializer-bundle": ">=1.1",
    "knplabs/knp-paginator-bundle": "^2.6",
    "nelmio/api-doc-bundle": "^2.13",
    "oro/doctrine-extensions": "dev-master",
    "sensio/distribution-bundle": "~4.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "sonata-project/admin-bundle": "3.23.0",
    "sonata-project/classification-bundle": "3.2.0",
    "sonata-project/doctrine-extensions": "~1",
    "sonata-project/doctrine-orm-admin-bundle": "^3.1",
    "sonata-project/exporter": "1.*",
    "sonata-project/jquery-bundle": "1.8.3",
    "sonata-project/user-bundle": "^3.2",
    "symfony/assetic-bundle": "^2.8",
    "symfony/monolog-bundle": "^3.0.2",
    "symfony/swiftmailer-bundle": "~2.3,>=2.3.10",
    "symfony/symfony": "2.8.*",
    "tilleuls/acl-sonata-admin-extension-bundle": "^2.0",
    "twbs/bootstrap": "3.0.*",
    "twig/twig": "^1.0||^2.0",
    "twitter/bootstrap": "3.*",
    "helios-ag/fm-elfinder-bundle": "~6",
    "sonata-project/core-bundle": "3.7.0",
    "knplabs/knp-menu-bundle": "2.1.3"
  },
  "require-dev": {
    "sensio/generator-bundle": "~3.0",
    "symfony/phpunit-bridge": "~2.7"
  },
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "config": {
    "bin-dir": "bin",
    "platform": {
      "php": "7.0"
    },
    "sort-packages": true,
    "component-dir": "web/assets"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": {
      "dev-master": "2.8-dev"
    }
  }
}
